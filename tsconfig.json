{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@screens": ["src/screens"],
      "@constants": ["src/constants"],
      "@utils": ["src/utils"],
      "@components": ["src/components"],
      "@config": ["src/config"],
      "@providers": ["src/providers"],
      "@services": ["src/services"],
      "@env": ["node_modules/react-native-dotenv"]
    }
  }
}
